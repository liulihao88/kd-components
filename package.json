{
  "name": "@kd/components",
  "alias": "kd-components",
  "version": "0.1.49",
  "private": false,
  "description": "科杰大数据组件库",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clean && npm run build:es && npm run build:umd && npm run build:theme",
    "build:es": "rollup --config build/rollup.esm.config.js",
    "build:umd": "rollup --config build/rollup.umd.config.js",
    "subBuild": "rollup --c build/rollup.config.sub.js",
    "build:theme": "npm run -C packages/theme-chalk build",
    "build:md": "node examples/src/genertate.js",
    "clean": "rimraf ./dist",
    "test:unit": "vue-cli-service test:unit",
    "clean:utils": "rimraf ./kd-utils",
    "build:utils": "npm run clean:utils && rollup --c build/rollup.config.utils.js",
    "all": "npm run build && npm run build:utils",
    "pub": "node ./bin/index.js",
    "build:exm": "vue-cli-service build"
  },
  "main": "dist/kd-components.esm.js",
  "module": "dist/kd-components.esm.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "keendata",
    "components"
  ],
  "author": "hrr",
  "peerDependencies": {
    "element-ui": "^2.0.0",
    "cron-parser": ">4.0.0",
    "vue": "<2.7.0",
    "lodash": "^4.17.21",
    "vue-i18n": "<9.0.0",
    "vue-multipane": "^0.9.5",
    "vue-virtual-scroll-list": "^2.3.3"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.3.3",
    "@rollup/plugin-alias": "^3.1.9",
