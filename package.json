{
  "name": "@kd/components",
  "alias": "kd-components",
  "version": "0.1.22",
  "private": false,
  "description": "科杰大数据组件库",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clean && npm run build:es && npm run build:umd && npm run subBuild",
    "build:es": "rollup --config build/rollup.esm.config.js",
    "build:umd": "rollup --config build/rollup.umd.config.js",
    "subBuild": "rollup --c build/rollup.config.sub.js",
    "clean": "rimraf ./dist",
    "test:unit": "vue-cli-service test:unit",
    "clean:utils": "rimraf ./kd-utils",
    "build:utils": "npm run clean:utils && rollup --c build/rollup.config.utils.js",
    "all": "npm run build && npm run build:utils",
    "publish": "node ./bin/index.js"
  },
  "main": "dist/lib/components/index.js",
  "module": "dist/es/components/index.mjs",
  "files": [
    "dist"
  ],
  "keywords": [
    "keendata",
    "components"
  ],
  "author": "hrr",
  "dependencies": {
    "shelljs": "^0.8.5"
  },
  "peerDependencies": {
    "element-ui": "^2.15.9",
    "core-js": "^3.8.3",
    "vue": "^2.6.14",
    "vue-i18n": "8.25.1",
    "vue-runtime-helpers": "^1.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "element-ui": "^2.15.9",
    "fs-extra": "^10.1.0",
    "jest": "^27.0.5",
    "node-sass": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.22",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "unplugin-vue-define-options": "^0.6.2",
    "vue": "^2.6.0",
    "vue-i18n": "8.25.1",
    "vue-template-compiler": "^2.6.14",
    "fast-glob": "^3.2.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
